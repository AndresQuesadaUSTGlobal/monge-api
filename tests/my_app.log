          <tr>\r\n                <td style=\"width: 20%;\">\r\n                    Ingresos mensuales estimados:\r\n                </td>\r\n                <td style=\"width: 25%;\">\r\n                   (X) Colones\r\n                </td>\r\n                <td style=\"width: 25%;\">\r\n                  ()  D&oacute;lares\r\n                </td>\r\n                <td style=\"width: 25%;\">\r\n                    Otros Ingresos\r\n                </td>\r\n            </tr>\r\n        </table>\r\n    </div>\r\n    <div id=\"DatosEmpresa\" class=\"borderOut\">\r\n        <table id=\"EncabezadoDatosEmpresas\" style=\"width: 100%; background-color: ButtonFace; font-weight:bold;\">\r\n            <tr>\r\n                <td align=\"left\">\r\n                    3. Datos de la empresa o lugar en que genera sus ingresos\r\n                </td>\r\n            </tr>\r\n        </table>\r\n        <table id=\"Trabajo\" style=\"width: 100%;\">\r\n            <tr>\r\n                <td style=\"width: 30%;\" class=\"borderOut\">\r\n                    Nombre de la empresa: DISTRIBUIDORA J Y F\r\n                </td>\r\n                <td style=\"width: 40%;\" class=\"borderOut\">\r\n                    Actividad o giro del negocio: N/A\r\n                </td>\r\n                <td style=\"width: 30%;\" class=\"borderOut\">\r\n                    Cargo que desempe&ntilde;a: Almacenista\r\n                </td>\r\n            </tr>\r\n        </table>\r\n        <table id=\"TrabajoDireccion\" style=\"width: 100%;\">\r\n            <tr>\r\n                <td style=\"width: 25%;\" class=\"borderOut\">\r\n                    <table>\r\n                        <tr>\r\n                            <td>\r\n                                Pa&iacute;s: Costa Rica \r\n                            </td>\r\n                        </tr>\r\n                        <tr>\r\n                            <td>\r\n                                <br />\r\n                            </t